FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (.1 0 0);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform (.1 0 0);
    }

    outlet
    {
        type            zeroGradient;
    }

    top
    {
    	type            noSlip;
        value           uniform (.1 0 0);
    }

    bottom
    {
    	type			noSlip;
    	value			uniform (0 0 0);
    }

    defaultFaces
    {
        type empty;
    }

}

// ************************************************************************* //
