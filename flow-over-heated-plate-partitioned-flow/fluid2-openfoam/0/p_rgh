FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 103500;

boundaryField
{
	inlet
	{
        type            fixedFluxExtrapolatedPressure;
        value           uniform 0;
	}
	outlet
	{
        type            fixedValue;
        value           uniform 103500;
	}
    top
    {
        type            zeroGradient;
    }

    bottom
    {
        type            zeroGradient;
    }
    defaultFaces
    {
    	type			empty;
    }
}

